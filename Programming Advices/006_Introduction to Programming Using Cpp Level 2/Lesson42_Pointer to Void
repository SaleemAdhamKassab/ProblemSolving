using System;
using System.Collections.Generic;
using System.Linq;
using System.Runtime.InteropServices;

namespace lab
{
    class Program
    {
        static void Main(string[] args)
        {
            unsafe
            {
                int a = 10;
                void* ptr = &a;
                cout << *(static_cast<int*>(ptr)) << endl;

            }
        }
    }
}

