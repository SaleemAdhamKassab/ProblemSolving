 static bool startWith(string s, string word)
    {
        return substring(s, 0, word.Length) == word ? true : false;
    }

static string substring(string s, int startIndex, int length = 0)
    {
        string result = "";
        int lastIndex = 0;

        if (length == 0)
            lastIndex = s.Length;
        else
            lastIndex = length;

        for (int i = startIndex; i < lastIndex; i++)
            result += s[i];

        return result;
    }
